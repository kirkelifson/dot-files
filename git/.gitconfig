[user]
        name = kirkelifson
        email = kirk@parodybit.net
[color]
	ui = auto
        branch = auto
        diff = auto
        status = auto
	interactive = auto
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[credential]
        helper = cache --timeout=3600
[merge]
        conflictstyle = diff3
[alias]
        st = status
        br = branch
        co = checkout
        lg = log --oneline --all --graph --color --date=relative --stat
[push]
        default = current
[core]
        editor = vim
